<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kpsoftwaresolutions.amadernatoreapp">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_f"
        android:label="@string/app_name"
        android:roundIcon="@drawable/govtlogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".activities.ZillaPotovumiActivity" />
        <activity android:name=".activities.UpozillaUnionActivity" />
        <activity android:name=".activities.DCCommentsActivity" />
        <activity android:name=".activities.HistoricalPlaceActivity" />
        <activity android:name=".activities.LanguageCultureActivity" />
        <activity android:name=".activities.FamousPersonsActivity" />
        <activity android:name=".activities.NatoreInWarActivity" />
        <activity android:name=".browser.BrowserActivity" />
    </application>

</manifest>